digraph mapLocation {
	compound=true;
	iteration [fontname=Helvetica,shape=box,fillcolor=lightgray,style="filled,rounded",label="Step"];
	start [fontname=Helvetica,shape=box,fillcolor=lightgray,style="filled,rounded",label="Start"];
	subgraph clusterWatch {
		label="Watch";
		fontname=Helvetica;
		style=filled;
		color=white;
		fillcolor=ghostwhite;
		labeljust=l;
		diffuse [shape=box,style="rounded,filled",fontname=Helvetica,color=black,fillcolor="0.0, 0.2, 1.0",label="Diffuse",URL="mapLocation_diffuse.svg"];
	}
	subgraph clusterRule {
		label="Rules";
		fontname=Helvetica;
		style=filled;
		color=white;
		fillcolor=ghostwhite;
		labeljust=l;
	}
	subgraph clusterInit {
		label="Initialize";
		fontname=Helvetica;
		style=filled;
		color=white;
		fillcolor=ghostwhite;
		labeljust=l;
	}
}
